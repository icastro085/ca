{
  "extends": ["eslint:recommended", "google", "plugin:react/recommended"],
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "react/prop-types": 0
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "Cookies": true,
    "router": true,
    "$": true
  }
}
